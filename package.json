  "homepage" : "https://flipper.netlify.com",
  "dependencies": {
    "@material-ui/core": "^3.3.2",
    "@material-ui/icons": "^3.0.1",
    "bignumber.js": "^7.2.1",
    "drizzle-react": "^1.2.0",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-scripts": "2.0.5",
    "truffle-contract": "^4.0.0-next.0",
